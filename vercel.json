{
  "version": 2,
  "name": "my-v0-project",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "includeFiles": ["**/*.ts"] }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/package.json"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://fluttertales.tech/mobileapi/v1",
    "NEXT_PUBLIC_APP_NAME": "School Management System",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0",
    "PAYU_MERCHANT_KEY": "aJWzRK",
    "PAYU_MERCHANT_SALT": "4wgOUmGrsrDLG96UTExMwejbXzSIVVj1",
    "PAYU_BASE_URL": "https://test.payu.in",
    "NEXT_PUBLIC_PAYU_BASE_URL": "https://test.payu.in",
    "DB_DATABASE": "sms"
  }
}
